{
  "compilerOptions": {
    "target": "es2016",  
    "module": "commonjs",     
    "esModuleInterop": true, 
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "strict": true,                               
    "skipLibCheck": true  ,                       
    "typeRoots": ["./node_modules/@types", "./types"],   
    "rootDir": "./src",
      "outDir": "./dist",
  }
}
  