model ComPaymentVoucherReason {
  id                Int                 @id @default(autoincrement())
  description       String
  isActive          Boolean             @default(true)
  blameUser         String
  createdAt         DateTime            @default(now())
  updatedAt         DateTime
  ComPaymentVoucher ComPaymentVoucher[]

